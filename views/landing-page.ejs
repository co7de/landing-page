<script>
    function validateForm() {
        // Retrieve form fields within the specific form
        var form = document.getElementById('ratingForm');
        var rating = form.querySelector('input[name="rating"]:checked');
        var name = form.querySelector('input[name="name"]');
        var profession = form.querySelector('input[name="profession"]');
        var city = form.querySelector('input[name="city"]');
        var review = form.querySelector('textarea[name="review"]');

        // Check if all required fields are filled
        if (!rating || !name.value || !profession.value || !city.value || !review.value) {
            alert("Please fill all the required fields.");
            return false; // Prevent form submission
        }

        return true; // Allow form submission
    }
</script>

