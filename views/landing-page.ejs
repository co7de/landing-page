<!--  Section: Book Appointment  -->
<div class="section appointment" id="appointment">
    <div class="container">
        <form id="add-apnt3" class="hidden" action="/online-appointment-booking" method="post">
            <div class="col-lg-12">
                <span class="bg-dark px-2 py-1 color-fff">Online</span>
                <h2 class="mb-1 mt-4 color-900">Book an Appointment</h2>
                <p class="lead">Book an appointment today and take the first step towards optimal health and wellness at
                    our esteemed clinic in Istanbul.</p>
            </div>
            <div class="row g-3">
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">ID Number</label>
                    <input type="text" class="form-control form-control-lg" placeholder="Enter here" required
                           name="idNumber">
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">First Name</label>
                    <input type="text" class="form-control form-control-lg" placeholder="Enter here" required
                           name="fName">
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">Last Name</label>
                    <input type="text" class="form-control form-control-lg" placeholder="Enter here" required
                           name="lName">
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">Your Email</label>
                    <input type="email" class="form-control form-control-lg" placeholder="Enter here" required
                           name="email">
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">Select Gender</label>
                    <select class="form-control form-control-lg" tabindex="-98" name="gender">
                        <option value="Male">Male</option>
                        <option value="Female">Female</option>
                    </select>
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">Mobile</label>
                    <input type="tel" class="form-control form-control-lg" placeholder="Enter here" required
                           name="tel">
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">Appointment Date</label>
                    <input id="date3" type="date" class="form-control form-control-lg" required
                           placeholder="Enter here"
                           name="date">
                </div>
                <div class="col-lg-4 col-md-6">
                    <label class="form-label">Appointment Hour</label>
                    <input id="time3" type="text" class="form-control form-control-lg" required
                           placeholder="Enter here" disabled
                           name="time" autocomplete="off">
                </div>
                <div class="col-sm-12">
                    <label class="form-label">Note you want to add</label>

                    <textarea rows="4" class="form-control no-resize" name="paragraph"
                              placeholder="Please type if you have any notes here..."></textarea>
                </div>
            </div>
            <div class="col-12" style="margin-top: 20px">
                <button type="submit" class="btn btn-lg btn-primary text-uppercase px-5">Book an Appointment</button>
            </div>
        </form> <!-- .row end -->
    </div>
</div>


<!--  Section: Testimonials  -->
<div class="section sliders-2 bg-body" style="height: 500px">
    <div class="container">
        <div class="testimonials-slider swiper swiper-container">
            <div class="swiper-wrapper">
                <% activeReviewsWithStars.forEach((review) => { %>
                    <div class="swiper-slide">
                        <div class="testimonial-wrap">
                            <div class="testimonial-item">
                                <h4 class="mb-0"><%= review.name %></h4>
                                <h6 class="text-muted mb-3"><%= review.profession %></h6>
                                <h6 class="lh-lg">
                                    <i class="fa fa-quote-left"></i> <%= review.review %> <i
                                            class="fa fa-quote-right"></i>
                                </h6>
                                <h6 class="text-muted mb-3"><%= review.city %></h6>
                                <div style="font-size: 30px" id="star-rating"><%- review.stars %></div>
                            </div>
                        </div>
                    </div>
                <% }) %>
            </div>
            <div class="swiper-pagination"></div>
        </div>
    </div>
</div>
<script src="/bundles/swiper.bundle.js"></script>
<script>
    // Testimonials slider
    new Swiper('.testimonials-slider', {
        speed: 600,
        loop: true,
        autoplay: {
            delay: 5000,
            disableOnInteraction: false
        },
        slidesPerView: 'auto',
        pagination: {
            el: '.swiper-pagination',
            type: 'bullets',
            clickable: true
        },
        breakpoints: {
            320: {
                slidesPerView: 1,
                spaceBetween: 20
            },
            1200: {
                slidesPerView: 2,
                spaceBetween: 20
            }
        }
    });
</script>


<!--  Section: rate us form  -->
<div id="rateSec" class="section">
    <div class="container">
        <form id="ratingForm" action="/rate" method="post" onsubmit="return validateForm()">
            <div class="row g-2">
                <div class="col-12">
                    <h3 class="mb-1 mt-4 color-900">Rate Us</h3>
                    <p class="lead">Your review is important to us</p>
                </div>
                <div style="border: 1px solid gold; border-radius: 5px; text-align: center" class="col-md-12 col-sm-6">
                    <div class="star-rating">
                        <input type="radio" id="star5" name="rating" value="5">
                        <label for="star5"></label>
                        <input type="radio" id="star4" name="rating" value="4">
                        <label for="star4"></label>
                        <input type="radio" id="star3" name="rating" value="3">
                        <label for="star3"></label>
                        <input type="radio" id="star2" name="rating" value="2">
                        <label for="star2"></label>
                        <input type="radio" id="star1" name="rating" value="1">
                        <label for="star1"></label>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="form-floating">
                        <input type="text" class="form-control" name="name" placeholder="Your Name">
                        <label>Your Name</label>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="form-floating">
                        <input type="text" class="form-control" name="profession" placeholder="Your Profession">
                        <label>Your Profession</label>
                    </div>
                </div>
                <div class="col-md-4 col-sm-6">
                    <div class="form-floating">
                        <input type="text" class="form-control" name="city" placeholder="Your City">
                        <label>Your City</label>
                    </div>
                </div>
                <div class="col-12">
                    <div class="form-floating">
                    <textarea class="form-control" name="review" placeholder="Please write your review of us here.."
                              style="height: 160px"></textarea>

                    </div>
                </div>
                <div class="col-12 mt-3">
                    <button type="submit" class="btn btn-lg btn-primary text-uppercase fs-6">Send</button>
                </div>
            </div><!-- .row end -->
        </form>
    </div>
</div>

